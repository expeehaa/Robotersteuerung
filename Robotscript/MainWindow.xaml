<Window x:Class="Robotscript.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Robotscript"
        mc:Ignorable="d"
        Title="Robotscriptwindow" Height="350" Width="525">
    <Grid>
        <Menu>
            <MenuItem Header="File">
                <MenuItem Header="New Robotscript"/>
                <MenuItem Header="Load Robotscript"/>
                <Separator/>
                <MenuItem Header="Close"/>
            </MenuItem>
            <MenuItem Header="Run script"/>
        </Menu>
    </Grid>
</Window>
